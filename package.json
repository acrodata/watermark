{
  "name": "watermark",
  "version": "0.0.0-NOT-USED",
  "engines": {
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve dev-app",
    "build": "ng build dev-app --base-href=/watermark/",
    "watch": "ng build dev-app --watch --configuration development",
    "test": "ng test dev-app",
    "lint": "ng lint --fix && npm run lint:scss",
    "lint:ts": "eslint \"projects/**/*.ts\" --fix",
    "lint:scss": "stylelint \"projects/**/*.scss\" --fix",
    "build:lib": "ng build watermark && cp README.md dist/watermark",
    "publish": "npm run build:lib && cd dist/watermark && npm publish",
    "deploy": "npm run build && ng deploy",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@acrodata/gui": "^2.9.0",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.17",
    "@angular/compiler": "^19.2.17",
    "@angular/core": "^19.2.17",
    "@angular/forms": "^19.2.17",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.17",
    "@angular/platform-browser-dynamic": "^19.2.17",
    "@angular/router": "^19.2.17",
    "@ng-matero/extensions": "^19.4.0",
    "html2canvas": "^1.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.19",
    "@angular/cli": "^19.2.19",
    "@angular/compiler-cli": "^19.2.17",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.0",
    "angular-eslint": "^19.0.0",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.0",
    "ng-packagr": "^19.2.2",
    "prettier": "^3.3.0",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.12.0"
  }
}
